{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "build": "yarn gen:theme-typings && tsc -b && vite build",
    "lint": "eslint --fix --ext ts,tsx ./src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "check-types": "tsc --noEmit",
    "gen:theme-typings": "chakra typegen ./src/theme/index.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.17.0",
    "@emotion/react": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.4.2",
    "@tanstack/react-table": "^8.21.3",
    "apexcharts": "^5.3.5",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "formik": "^2.4.9",
    "moment": "^2.30.1",
    "quilljs": "^0.18.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.57.0",
    "react-quill-new": "^3.8.3",
    "react-router-dom": "6.23.1",
    "react-select": "^5.8.0",
    "recoil": "^0.7.7",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.21.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.0",
    "@svgr/core": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/typescript-estree": "^6.4.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "commitizen": "^4.3.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  }
}
